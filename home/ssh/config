Include ~/.ssh/config.local

Host *
  IgnoreUnknown UseKeychain

Host nigo
  User mizuki
  HostName nigo
  ForwardAgent yes
  IdentityFile ~/.ssh/id_ed25519
  AddKeysToAgent yes
  PreferredAuthentications publickey

Host lydie.charlese.dev
  User ubuntu
  HostName lydie.charlese.dev
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519_oracle

  IdentitiesOnly yes
  PreferredAuthentications publickey

Host suelle.charlese.dev
  User opc
  HostName suelle.charlese.dev
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519_oracle_linux
  IdentitiesOnly yes
  PreferredAuthentications publickey


Host github.com-hhosu107
  HostName github.com
  User git
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519
  IdentitiesOnly yes
  PreferredAuthentications publickey

  Include config.local

Host himari
  HostName himari.tirr.dev
  ForwardAgent yes
  AddKeysToAgent yes
